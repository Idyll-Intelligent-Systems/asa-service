{
  "watch": [
    "backend.js",
    "data/",
    ".env.development"
  ],
  "ext": "js,json,csv,env",
  "ignore": [
    "node_modules/",
    "logs/",
    "*.log"
  ],
  "nodeArgs": ["--env-file=.env.development"],
  "env": {
    "NODE_ENV": "development"
  },
  "delay": 1000,
  "verbose": true,
  "restartable": "rs"
}
